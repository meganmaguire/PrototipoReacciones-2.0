<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="772.0" prefWidth="1115.0" spacing="46.0" styleClass="pane" stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mmaguire.prototiporeacciones2.controller.HistorialController">
   <padding>
      <Insets bottom="40.0" left="54.0" right="54.0" top="40.0" />
   </padding>
   <children>
      <Label styleClass="title" text="%hist.titulo" />
      <HBox prefHeight="618.0" prefWidth="971.0" spacing="46.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="tablaHistorial" prefHeight="582.0" prefWidth="973.0" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="columnaNro" editable="false" minWidth="55.0" prefWidth="-1.0" sortable="false" text="%hist.numero" />
                <TableColumn fx:id="columnaTiempo" editable="false" minWidth="300.0" prefWidth="-1.0" sortable="false" text="%hist.tiempo" />
                  <TableColumn fx:id="columnaComponentes" editable="false" minWidth="250.0" prefWidth="-1.0" sortable="false" text="%hist.comp" />
                  <TableColumn fx:id="columnaCantReacciones" editable="false" minWidth="100.0" prefWidth="-1.0" sortable="false" text="%hist.reacciones" />
                  <TableColumn fx:id="columnaPasos" editable="false" minWidth="150.0" prefWidth="-1.0" sortable="false" text="%hist.pasos" />
                  <TableColumn fx:id="columnaGrafico" editable="false" minWidth="60.0" prefWidth="-1.0" sortable="false" text="%hist.grafico" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
   </children>
</VBox>
